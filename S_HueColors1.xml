<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>
    <stateVariable sendEvents="no">
      <name>Preset1</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>Preset2</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>Preset3</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>Preset4</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>Preset5</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>Preset6</name>
      <dataType>string</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>ColorTemperature</name>
      <dataType>float</dataType>
	  <allowedValueRange>
        <minimum>0</minimum>
        <maximum>100</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>Hue</name>
      <dataType>float</dataType>
	  <allowedValueRange>
        <minimum>0</minimum>
        <maximum>100</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>Saturation</name>
      <dataType>float</dataType>
	  <allowedValueRange>
        <minimum>0</minimum>
        <maximum>100</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_UpDown</name>
      <dataType>string</dataType>
	  <allowedValueList>
        <allowedValue></allowedValue>
        <allowedValue>Down</allowedValue>
        <allowedValue>Up</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Preset</name>
      <dataType>int</dataType>
	  <allowedValueRange>
        <minimum>1</minimum>
        <maximum>6</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Float100</name>
      <dataType>float</dataType>
	  <allowedValueRange>
        <minimum>0</minimum>
        <maximum>100</maximum>
      </allowedValueRange>
    </stateVariable>
  </serviceStateTable>
  <actionList>
    <action>
      <name>ChangeColor</name>
      <argumentList>
        <argument>
          <name>Temperature</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_UpDown</relatedStateVariable>
        </argument>
        <argument>
          <name>Saturation</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_UpDown</relatedStateVariable>
        </argument>
        <argument>
          <name>Hue</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_UpDown</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetPreset</name>
      <argumentList>
        <argument>
          <name>Preset</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Preset</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>ProgramPreset</name>
      <argumentList>
        <argument>
          <name>Preset</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Preset</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetColorTemperature</name>
      <argumentList>
        <argument>
          <name>newColorTemperature</name>
          <direction>in</direction>
          <relatedStateVariable>ColorTemperature</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetHue</name>
      <argumentList>
        <argument>
          <name>newHue</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Float100</relatedStateVariable>
		</argument>
      </argumentList>
    </action>
    <action>
      <name>SetHueSaturation</name>
      <argumentList>
        <argument>
          <name>newHue</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Float100</relatedStateVariable>
        </argument>
        <argument>
          <name>newSaturation</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Float100</relatedStateVariable>
		</argument>
      </argumentList>
    </action>
    <action>
      <name>SetSaturation</name>
      <argumentList>
        <argument>
          <name>Saturation</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Float100</relatedStateVariable>
        
		</argument>
      </argumentList>
    </action>
  </actionList>
</scpd>
